<div class="navbar">
    <%= link_to "Alquilapp", root_path, class:"logo" %>
    
    <% if current_user %>
        <%= link_to "Cerrar Sesión",
            destroy_user_session_path, 
            :method => :delete,
            class: "nav-link"%>

        <%= link_to "Editar Perfil",
            edit_user_registration_path, 
            class: "nav-link"%>
    
    <% else %>
        <%= link_to "Iniciar Sesión",
